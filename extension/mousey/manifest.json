{
  "name":"Mousey",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Show/Hide Mousey",
    "default_icon": "cursor.png"
  },
  "web_accessible_resources": [
    "cursor.png"
  ],
  "manifest_version": 2
}
